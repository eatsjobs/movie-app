{
  "homepage": "https://eatsjobs.github.io/movie-app/",
  "name": "org.apache.cordova.sampleapp",
  "displayName": "sample-app",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --env.APP_ENV=web --open --config webpack.dev.js",
    "start:hybrid": "webpack-dev-server --env.APP_ENV=hybrid --env.PLATFORM=android --config webpack.dev.js",
    "build": "webpack --env.APP_ENV=web --config webpack.prod.js",
    "build:dev": "webpack --env.APP_ENV=web --config webpack.dev.js",
    "build:dev:hybrid": "webpack --env.APP_ENV=hybrid --config webpack.dev.js",
    "build:hybrid": "webpack --env.APP_ENV=hybrid --config webpack.prod.js",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "code-push": "^2.0.1-beta",
    "cordova-android": "^6.3.0",
    "cordova-ios": "^4.5.3",
    "cordova-plugin-code-push": "^1.10.0-beta",
    "cordova-plugin-device": "^1.1.7",
    "cordova-plugin-dialogs": "^1.3.4",
    "cordova-plugin-file": "^5.0.0",
    "cordova-plugin-file-transfer": "^1.7.0",
    "cordova-plugin-httpd": "^0.9.3",
    "cordova-plugin-network-information": "^1.3.4",
    "cordova-plugin-whitelist": "^1.3.3",
    "cordova-plugin-zip": "^3.1.0",
    "onsenui": "^2.8.2",
    "rc-slider": "^8.4.1",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-hot-loader": "^3.1.3",
    "react-onsenui": "^1.9.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "url-search-params": "^0.10.0",
    "whatwg-fetch": "^2.0.3",
    "workbox-webpack-plugin": "^2.1.2"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-network-information": {},
      "cordova-plugin-file": {},
      "cordova-plugin-code-push": {},
      "cordova-plugin-httpd": {}
    },
    "platforms": [
      "ios",
      "android"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.2.3",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "gh-pages": "^1.1.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.1",
    "webpack-pwa-manifest": "^3.3.2"
  }
}
